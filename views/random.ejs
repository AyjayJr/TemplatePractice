<%- include('../partials/head.ejs') %>

<body>
	<%- include('../partials/navbar.ejs') %>

	<h1>Your random number is: <%= num %> </h1>
	<!-- This is an example of control flow statements in EJS -->
	<% if (num % 2 === 0) { %>
	<h1>That is an even number</h1>
	<% } else { %>
	<h1>That is an odd number</h1>
	<% } %>
</body>
</html>